{
  "name": "TabTime",
  "description": "Tab time management.",
  "version": "0.1",
  "permissions": [
    "webNavigation",
    "tabs",
    "storage",
    "idle"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "TabTime",
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2
}